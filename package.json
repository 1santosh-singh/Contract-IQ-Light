{
  "name": "contract-ai-app",
  "version": "0.1.0",
  "private": true,
  "description": "AI-Powered Contract Analysis Platform with Glassmorphic UI",
  "keywords": [
    "contract-analysis",
    "ai",
    "nextjs",
    "tailwindcss",
    "glassmorphic"
  ],
  "author": "Contract IQ Team",
  "license": "MIT",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev:frontend": "cd packages/frontend && npm run dev",
    "dev:backend": "cd packages/backend && uvicorn main:app --reload --port 8000",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd packages/frontend && npm run build",
    "build:backend": "cd packages/backend && python -m build",
    "start": "npm run start:frontend",
    "start:frontend": "cd packages/frontend && npm run start",
    "start:backend": "cd packages/backend && uvicorn main:app --host 0.0.0.0 --port 8000",
    "lint": "npm run lint:frontend",
    "lint:frontend": "cd packages/frontend && npm run lint",
    "type-check": "npm run type-check:frontend",
    "type-check:frontend": "cd packages/frontend && npm run type-check"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
